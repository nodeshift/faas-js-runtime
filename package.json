{
  "name": "faas-js-runtime",
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodeshift/faas-js-runtime.git"
  },
  "author": "Red Hat, Inc.",
  "license": "Apache-2.0",
  "engines": {
    "node": "^20 || ^18 || ^16"
  },
  "type": "commonjs",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore .",
    "test": "npm run test:source && npm run test:types",
    "test:source": "nyc --reporter=lcovonly tape test/test*.js | colortape",
    "test:types": "tsd",
    "pretest": "npm run lint",
    "postinstall": "npx @cyclonedx/cyclonedx-npm --omit dev --package-lock-only --output-file sbom.json"
  },
  "description": "A Node.js framework for executing arbitrary functions in response to HTTP or cloud events",
  "files": [
    "index.d.ts",
    "index.js",
    "lib",
    "bin",
    "sbom.json"
  ],
  "bugs": {
    "url": "https://github.com/nodeshift/faas-js-runtime/issues"
  },
  "types": "index.d.ts",
  "bin": "./bin/cli.js",
  "dependencies": {
    "cloudevents": "^7.0.0",
    "commander": "^10.0.0",
    "death": "^1.1.0",
    "fastify": "^4.17.0",
    "js-yaml": "^4.1.0",
    "node-os-utils": "^1.3.7",
    "overload-protection": "^1.2.3",
    "prom-client": "^14.1.1",
    "qs": "^6.11.1"
  },
  "devDependencies": {
    "@cyclonedx/cyclonedx-npm": "^1.12.0",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.58.0",
    "colortape": "^0.1.2",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.7.0",
    "nyc": "^15.1.0",
    "supertest": "^6.3.1",
    "tape": "^5.6.3",
    "tsd": "^0.28.1",
    "typescript": "^4.9.5"
  },
  "tsd": {
    "directory": "test/types",
    "typingsFile": "index.d.ts"
  }
}
