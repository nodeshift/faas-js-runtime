{
  "name": "faas-js-runtime",
  "version": "0.6.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/boson-project/faas-js-runtime.git"
  },
  "author": "Red Hat, Inc.",
  "license": "Apache-2.0",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore .",
    "test": "npm run test:source && npm run test:types",
    "test:source": "nyc tape test/*.js | tap-spec",
    "test:types": "tsd",
    "pretest": "npm run lint"
  },
  "description": "A Node.js framework for executing arbitrary functions in response to HTTP or cloud events",
  "files": [
    "index.d.ts",
    "index.js",
    "lib",
    "bin"
  ],
  "types": "index.d.ts",
  "bin": "./bin/cli.js",
  "dependencies": {
    "chalk": "^4.1.0",
    "cloudevents": "^4.0.2",
    "commander": "^6.1.0",
    "death": "^1.1.0",
    "fastify": "^3.3.0",
    "overload-protection": "^1.1.0",
    "qs": "^6.9.0"
  },
  "devDependencies": {
    "@types/node": "^15.0.3",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^8.3.0",
    "nyc": "^14.1.1",
    "supertest": "^4.0.2",
    "tap-spec": "^5.0.0",
    "tape": "^4.11.0",
    "tsd": "^0.15.1",
    "typescript": "^4.2.4"
  },
  "standardx": {
    "ignore": [
      "lib/context.js"
    ]
  },
  "tsd": {
    "directory": "test/types",
    "typingsFile": "index.d.ts"
  }
}
